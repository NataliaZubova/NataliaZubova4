  <data name="sqlInsertCommand1.CommandText" xml:space="preserve">
    <value>INSERT INTO [Покупатели] ([КодПокуп], [Фамилия], [Имя], [Отчество], [Телефон], [Индекс], [Страна], [Город], [Область], [Адрес], [Предпр], [Кредит]) VALUES (@КодПокуп, @Фамилия, @Имя, @Отчество, @Телефон, @Индекс, @Страна, @Город, @Область, @Адрес, @Предпр, @Кредит);
