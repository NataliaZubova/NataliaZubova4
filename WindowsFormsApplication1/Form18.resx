  <data name="sqlUpdateCommand1.CommandText" xml:space="preserve">
    <value>UPDATE [Покупатели] SET [КодПокуп] = @КодПокуп, [Фамилия] = @Фамилия, [Имя] = @Имя, [Отчество] = @Отчество, [Телефон] = @Телефон, [Индекс] = @Индекс, [Страна] = @Страна, [Город] = @Город, [Область] = @Область, [Адрес] = @Адрес, [Предпр] = @Предпр, [Кредит] = @Кредит WHERE (([КодПокуп] = @Original_КодПокуп) AND ((@IsNull_Фамилия = 1 AND [Фамилия] IS NULL) OR ([Фамилия] = @Original_Фамилия)) AND ((@IsNull_Имя = 1 AND [Имя] IS NULL) OR ([Имя] = @Original_Имя)) AND ((@IsNull_Отчество = 1 AND [Отчество] IS NULL) OR ([Отчество] = @Original_Отчество)) AND ((@IsNull_Телефон = 1 AND [Телефон] IS NULL) OR ([Телефон] = @Original_Телефон)) AND ((@IsNull_Индекс = 1 AND [Индекс] IS NULL) OR ([Индекс] = @Original_Индекс)) AND ((@IsNull_Страна = 1 AND [Страна] IS NULL) OR ([Страна] = @Original_Страна)) AND ((@IsNull_Город = 1 AND [Город] IS NULL) OR ([Город] = @Original_Город)) AND ((@IsNull_Область = 1 AND [Область] IS NULL) OR ([Область] = @Original_Область)) AND ((@IsNull_Адрес = 1 AND [Адрес] IS NULL) OR ([Адрес] = @Original_Адрес)) AND ((@IsNull_Предпр = 1 AND [Предпр] IS NULL) OR ([Предпр] = @Original_Предпр)) AND ((@IsNull_Кредит = 1 AND [Кредит] IS NULL) OR ([Кредит] = @Original_Кредит)));
SELECT КодПокуп, Фамилия, Имя, Отчество, Телефон, Индекс, Страна, Город, Область, Адрес, Предпр, Кредит FROM Покупатели WHERE (КодПокуп = @КодПокуп)</value>
